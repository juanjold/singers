<p id="notice"><%= notice %></p>

<div class="container">
<p>
  <b>Name:</b>
  <%= @singer.name %>
</p>

<p>
  <b>Lastname:</b>
  <%= @singer.lastname %>
</p>

<p>
  <b>Voicepart:</b>
  <%= @singer.voicepart %>
</p>

<p>
  <b>Skills:</b>
  <%= @singer.skills %>
</p>

<p>
  <b>Bio:</b>
  <%= @singer.bio %>
</p>

<p>
  <b>Email:</b>
  <%= @singer.email %>
</p>

<p>
  <b>Number:</b>
  <%= @singer.number %>
</p>

<p>
  <b>Status:</b>
  <%= @singer.status %>
</p>

<p>
  <b>Performing: </b>
  <% if @singer.performing == true %>
  <%= "Yes"%>
  <% else %>
  <%= "No"%>
  <% end %>
</p>

<p>
  <b>Points:</b>
  <%= @singer.points %>
</p>

<p>
  <b>Id:</b>
  <%= @singer.id %>
</p>

<p>
  <b>Year:</b>
  <%= @singer.year %>
</p>
</div>

<div class="container" align="center">
	<footer>
		<div class="btn">
		<%= link_to 'Edit', edit_singer_path(@singer) %> 
		</div>
		<div class="btn">
			<%= link_to 'Back', singers_path %>
		</div>
	</footer>
</div>
